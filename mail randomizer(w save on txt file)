from random import randint
import random
prenoms=[,//entrez des pr√©noms quelconques// ]

for boucle in range(200) :
    pseudo_number = str(randint(1,100))
    Pseudo = [random.choice(prenoms)]
    random_pseudo = random.choice(Pseudo)
    stl = '_'.join([str(random_pseudo),pseudo_number])
    
    fichier = open("C:/Users/maheb/Documents/mails.txt", "a")
    fichier.write(stl+"@gmail.com\n")
    fichier.close()
